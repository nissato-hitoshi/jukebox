<% extend './layouts/layout' %>
<% block 'local_javascripts' : %>
<script src="javascripts/jukebox.js"></script>
<% end %>
<div id="player"></div>
<input type="hidden" id="objectId" value="" />
<input type="hidden" id="videoId" value="" />
<div class="btn-group" role="group" aria-label="...">
  <div class="btn-group" role="group">
    <button id="play" class="btn btn-default" >
      <span class="glyphicon glyphicon-play" aria-hidden="true"></span>
    </button>
  </div>
  <div class="btn-group" role="group">
    <button id="pause" class="btn btn-default" >
      <span class="glyphicon glyphicon-pause" aria-hidden="true"></span>
    </button>
  </div>
</div>
